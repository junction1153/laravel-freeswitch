{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-simplexml": "*",
        "aws/aws-sdk-php": "~3.0",
        "doctrine/dbal": "^3.4",
        "firebase/php-jwt": "^6.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.6.11",
        "laravel/fortify": "^1.21",
        "laravel/framework": "^10",
        "laravel/horizon": "*",
        "laravel/sanctum": "^3.2",
        "laravel/slack-notification-channel": "^3.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^4.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "linfo/linfo": "^4.0",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "nwidart/laravel-modules": "^10.0",
        "predis/predis": "^2.0",
        "propaganistas/laravel-phone": "^5.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-webhook-client": "^3.2",
        "symfony/http-client": "^6.4",
        "symfony/postmark-mailer": "^6.4",
        "tightenco/ziggy": "^1.8"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php",
            "public/resources/classes/cache.php",
            "public/resources/classes/event_socket.php",
            "public/resources/tcpdf/tcpdf.php",
            "public/resources/fpdi/fpdi.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
